class Solution {
    public int maxSubarraySum(int[] arr, int k) {
        // Code here
        int l=0,r=0;
        int maxsum=0;
        int sum=0;
        for(r=0;r<arr.length;r++){
            sum+=arr[r];
            if(r-l+1==k){
                maxsum=Math.max(sum,maxsum);
                sum-=arr[l];
                l++;
            }
        }
        return maxsum;
    }
}
